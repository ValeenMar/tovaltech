{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*"
    ]
  },
  "routes": [
    {
      "route": "/admin/*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/api/admin/*",
      "allowedRoles": [
        "authenticated"
      ]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    }
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}